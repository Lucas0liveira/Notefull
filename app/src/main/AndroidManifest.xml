<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.notefull">

    <uses-permission android:name="android.permission.VIBRATE">
    </uses-permission>


    <application
        android:allowBackup="true"
        android:icon="@drawable/rectangle_2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NoteActivity"></activity>
        <activity android:name=".NewNoteActivity"></activity>
        <activity android:name=".CadastroActivity"></activity>
        <activity android:name=".EdtNotesActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <receiver android:name=".Lembrete"></receiver>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>